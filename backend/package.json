{
  "name": "backend",
  "version": "1.0.0",
  "description": "An outpass Management System",
  "main": "dist/index.js",
  "scripts": {
    "dev:server": "nodemon --watch '*.ts' --exec 'ts-node --transpile-only' ./index.ts",
    "dev:worker": "nodemon --watch '*.ts' --exec 'ts-node --transpile-only' worker-entry.ts",
    "dev": "npm run dev:server",
    "postinstall": "prisma generate",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "worker": "ts-node --transpile-only worker-entry.ts"
  },
  "author": "SreeCharan Desu",
  "license": "MIT",
  "devDependencies": {
    "@prisma/client": "^7.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^6.4.15",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.4",
    "prisma": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@types/pdfkit": "^0.17.4",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.65.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "google-auth-library": "^10.5.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.11",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
