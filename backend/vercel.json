{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/index.ts"
    },
    {
        "source": "/(health|)",
        "destination": "/api/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/cron/stock-alerts",
      "schedule": "* * * * *"
    }
  ]
}
